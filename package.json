{
  "name": "@daotl/nuxt-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.9.0",
  "description": "A Nuxt 3 + TypeScript starter template",
  "homepage": "https://github.com/daotl/fes-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nuxt-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "build:lambda": "cross-env NITRO_PRESET=lambda nuxi build",
    "generate": "nuxi generate",
    "graphql-gen": "npm run graphql-gen-ops && npm run graphql-codegen",
    "graphql-gen-ops": "gqlg --schemaFilePath ./graphql/generated/schema.gql --destDirPath ./graphql/generated/ops --depthLimit 100",
    "graphql-codegen": "graphql-codegen -c ./graphql/codegen.yml",
    "start": "node .output/server/index.mjs",
    "serverless:deploy": "serverless deploy",
    "serverless:domain": "serverless create_domain",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "prepare": "husky install",
    "postinstall": "nuxi prepare"
  },
  "prettier": "@daotl/prettier-config",
  "dependencies": {
    "@iconify-json/carbon": "^1.1.9",
    "@iconify-json/twemoji": "^1.1.5",
    "@urql/vue": "^1.0.2",
    "apollo-server-core": "^3.10.3",
    "class-validator": "^0.13.2",
    "element-plus": "^2.2.19",
    "graphql": "^15.3.0",
    "graphql-playground-html": "^1.6.30",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.0.23",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "type-graphql": "^1.1.1",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^1.0.9"
  },
  "devDependencies": {
    "@daotl/eslint-config-vue": "^0.3.32",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/near-operation-file-preset": "^2.4.3",
    "@graphql-codegen/typed-document-node": "^2.3.5",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.5",
    "@graphql-codegen/typescript-vue-urql": "^2.3.5",
    "@nuxtjs/color-mode": "^3.1.8",
    "@pinia/nuxt": "^0.4.3",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.11.5",
    "@unocss/nuxt": "^0.46.0",
    "@vueuse/nuxt": "^9.4.0",
    "eslint": "^8.26.0",
    "gql-generator": "^1.0.18",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.12",
    "nuxt-typed-router": "^1.2.3",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.1.8"
  }
}
